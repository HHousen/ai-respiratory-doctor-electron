{
  "name": "ai-respiratory-doctor-electron",
  "productName": "AI Respiratory Doctor Desktop",
  "version": "1.0.0",
  "description": "ai-respiratory-doctor-electron",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist-wl": "yarn compile && electron-builder -p always -wl",
    "dist-m": "yarn compile && electron-builder -p always -m",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "keywords": [],
  "author": {
    "name": "Hayden Housen",
    "email": "hayden@haydenhousen.com"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "materialize-css": "^1.0.0",
    "source-map-support": "^0.5.12",
    "sweetalert2": "^8.17.1"
  },
  "devDependencies": {
    "css-loader": "^3.2.0",
    "electron": "7.2.4",
    "electron-builder": "^21.0.11",
    "electron-webpack": "^2.7.4",
    "material-design-icons-iconfont": "^5.0.1",
    "node-sass": "^4.13.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "webpack": "~4.35.3"
  },
  "electronWebpack": {
    "staticSourceDirectory": "src/static",
    "main": {
      "extraEntries": [
        "@/preload.js"
      ],
      "webpackConfig": "custom.additions.webpack.js"
    },
    "renderer": {
      "webpackConfig": "custom.additions.webpack.js"
    }
  },
  "build": {
    "extraResources": [
      "src/predict.py",
      "src/static",
      "src/models"
    ]
  }
}
